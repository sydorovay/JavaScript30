{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "insertPragma": false,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "printWidth": 80,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "requirePragma": false,
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "htmlWhitespaceSensitivity": "css",
  "overrides": [
    {
      "files": ["*.html"],
      "options": {
        "semi": false,
        "singleQuote": true,
        "printWidth": 0
      }
    },
    {
      "files": ["*.json"],
      "options": {
        "semi": false,
        "singleQuote": false,
        "printWidth": 120
      }
    }
  ]
}